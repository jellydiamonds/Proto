package com.jellydiamonds.android.metier;

public enum GemLight {
	
	EMPTY(0),
	Daylight(1),
	Fluorescent_light(2),
	Incandescent_light(3);
	
	protected int light;
	
	GemLight( int light )
	{
		this.light = light;
	}
	
	public Integer getValue()
	{
		return this.light;
	}
	
	public void setValue( Integer value )
	{
		if( 	(value <= 0 ) || 
				( value >= values().length ) )
			this.light = 0;
		else
			this.light = value;
	}
}
