package com.jellydiamonds.android.comm;

import java.util.Collection;

import org.json.JSONArray;
import org.json.JSONObject;

import com.jellydiamonds.android.metier.GemID;


public class JellySerialize {

	JSONArray SerializeJellyCollection( Collection<GemID> collection )
	{
		JSONArray l_serializedCollection = new JSONArray();
		JSONObject l_serialized_gemid = null;
		
		for( GemID l_tmp_gemid : collection )
		{
			l_serialized_gemid = new JSONObject();
			
			l_serialized_gemid.put("reference", value);
			l_serialized_gemid.put("color", value);
			l_serialized_gemid.put("mass", value);
			l_serialized_gemid.put("sizeX", value);
			l_serialized_gemid.put("sizeY", value);
			l_serialized_gemid.put("sizeZ", value);
			l_serialized_gemid.put("comments", value);
			l_serialized_gemid.put("priceCurrency", value);
			l_serialized_gemid.put("priceValue", value);
			l_serialized_gemid.put("supplierID", value);
			l_serialized_gemid.put("creationDate", value);
			l_serialized_gemid.put("photoLink", value);
			l_serialized_gemid.put("species", value);
			l_serialized_gemid.put("shape", value);
			l_serialized_gemid.put("cut", value);
			l_serialized_gemid.put("clarity", value);
			l_serialized_gemid.put("", value);
			l_serialized_gemid.put("", value);
			l_serialized_gemid.put("", value);
			l_serialized_gemid.put("", value);
			
		}
		
		
		return l_serializedCollection;
	}
}
